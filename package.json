{
  "name": "truenest",
  "version": "0.0.1",
  "description": "ephemeral P2P screenshot sharing",
  "main": "index.js",
  "scripts": {
    "test": "standard",
    "build": "npm run build-css && npm run build-js",
    "build-css": "stylus -u nib css/main.styl -o public/ -c",
    "build-js": "browserify client > public/bundle.js",
    "start": "node server",
    "watch": "npm run watch-css & npm run watch-js & DEBUG=instant* nodemon server",
    "watch-css": "stylus -u nib css/main.styl -o public/ -w",
    "watch-js": "watchify client -o public/bundle.js -dv"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/truenest/truenest.git"
  },
  "author": "jme",
  "license": "GNU",
  "homepage": "https://github.com/yjoshuaz/truenest/blob/master/README.md",
  "dependencies": {
    "body-parser": "^1.14.2",
    "connect-busboy": "0.0.2",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "fd-slicer": "^1.0.1",
    "http-post": "^0.1.1",
    "monk": "^1.0.1",
    "mongodb": "^1.4.4",
    "multiparty": "^4.1.2",
    "request": "^2.67.0",
    "uuid": "^2.0.1",
    "webtorrent": "0.70.0"
  },
  "devDependencies": {
    "nib": "^1.1.0",
    "nodemon": "^1.8.1",
    "stylus": "^0.53.0",
    "watchify": "^3.6.1"
  }
}
